<h6 class="card-header">{{ i18n locale "socialsLabel"}}</h6>
<form class="card-body">
  {{#each links as |link|}}
    <div class="row">
      <div class="form-group col-md-4">
        {{review-field
          original=link.original.provider
          updated=link.updated.provider
          toggleField=toggleField
          fieldKey="socialLinks"
          subField="provider"
          enabled=payload.socialLinks
        }}
      </div>
      <div class="form-group col-md-8">
        {{review-field
          original=link.original.url
          updated=link.updated.url
          toggleField=toggleField
          fieldKey="socialLinks"
          subField="url"
          enabled=payload.socialLinks
        }}
      </div>
    </div>
  {{/each}}
</form>
<div class="card-footer"></div>
